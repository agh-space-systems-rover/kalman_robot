  <version>0.0.0</version>
  <description>configuration + launch files for robot_localization and RTAB-Map</description>
  <maintainer email="63957587+rayferric@users.noreply.github.com">dupa</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_python</buildtool_depend>

  <exec_depend>ros2launch</exec_depend>
  <exec_depend>robot_localization</exec_depend>
  <exec_depend>compressed_image_transport</exec_depend>
  <exec_depend>compressed_depth_image_transport</exec_depend>
  <exec_depend>rtabmap_ros</exec_depend>
  <exec_depend>rtabmap_odom</exec_depend>
  <exec_depend>python3-jinja2</exec_depend>
  
  <exec_depend>point_cloud_utils</exec_depend>

  <test_depend>ament_lint_auto</test_depend>
